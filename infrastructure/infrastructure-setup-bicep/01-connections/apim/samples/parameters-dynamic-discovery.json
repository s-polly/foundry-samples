{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "projectResourceId": {
      "value": "/subscriptions/YOUR-SUBSCRIPTION-ID/resourceGroups/YOUR-RG/providers/Microsoft.CognitiveServices/accounts/YOUR-AI-FOUNDRY-ACCOUNT/projects/YOUR-PROJECT"
    },
    "apimResourceId": {
      "value": "/subscriptions/YOUR-SUBSCRIPTION-ID/resourceGroups/YOUR-APIM-RG/providers/Microsoft.ApiManagement/service/YOUR-APIM-SERVICE"
    },
    "apiName": {
      "value": "YOUR-API-NAME"
    },
    "connectionName": {
      "value": "YOUR-CONNECTION-NAME"
    },
    "authType": {
      "_comment": "Set to ApiKey or ProjectManagedIdentity",
      "value": "ApiKey"
    },
    "isSharedToAll": {
      "value": false
    },
    "deploymentInPath": {
      "_comment": "true or false - Set based on setup guide to avoid 404 errors in agents",
      "value": "false"
    },
    "inferenceAPIVersion": {
      "_comment": "Set a proper value or leave empty if your APIM backend does not need it. Set based on setup guide to avoid 404 errors in agents",
      "value": ""
    },
    "deploymentAPIVersion": {
      "value": ""
    },
    "listModelsEndpoint": {
      "value": "/deployments"
    },
    "getModelEndpoint": {
      "value": "/deployments/{deploymentName}"
    },
    "deploymentProvider": {
      "value": "AzureOpenAI"
    }
  }
}