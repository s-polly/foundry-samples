      "value": {
        "type": "api_key",
        "name": "Authorization",
        "format": "Bearer {api_key}"
      }
    },
    "customHeaders": {
      "value": {
        "YOUR-HEADER-NAME-1": "YOUR-HEADER-VALUE-1",
