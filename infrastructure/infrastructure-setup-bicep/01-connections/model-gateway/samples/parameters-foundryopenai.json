      "value": [
        {
          "name": "YOUR-DEPLOYMENT-NAME-1",
          "properties": {
            "model": {
              "name": "YOUR-MODEL-NAME-1",
              "version": "YOUR-MODEL-VERSION-1",
              "format": "OpenAI"
            }
          }
        },
        {
          "name": "YOUR-DEPLOYMENT-NAME-2",
          "properties": {
            "model": {
              "name": "YOUR-MODEL-NAME-2",
              "version": "YOUR-MODEL-VERSION-2",
              "format": "OpenAI"
            }
          }
        }
      ]
